@model EntityModels.Story
@{
    ViewData["Title"] = "ViewStory";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <link href="http://cdnjs.cloudflare.com/ajax/libs/summernote/0.8.18/summernote.css" rel="stylesheet" />
<div class="row">
    <div class="col-md-12">
        <h4 style="white-space: break-spaces">@Model.Title</h4>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        @Html.Raw(Model.StoryContent)
    </div>
</div>

